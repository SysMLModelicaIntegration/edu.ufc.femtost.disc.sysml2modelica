               menubarPath="edu.ufc.femtost.disc.sysml4modelica.papyrus.ui.menu.sysml4modelica/SysML4ModelicaSeparator">
         </action>
         <action
               class="edu.ufc.femtost.disc.sysml4modelica.papyrus.ui.actions.ExportAction"
               id="edu.ufc.femtost.disc.sysml4modelica.papyrus.ui.exportaction"
               label="Export to Modelica model"
               menubarPath="edu.ufc.femtost.disc.sysml4modelica.papyrus.ui.menu.sysml4modelica/SysML4ModelicaSeparator">
         </action>
         <action
               class="edu.ufc.femtost.disc.sysml4modelica.papyrus.ui.actions.GenerateAction"
               id="edu.ufc.femtost.disc.sysml4modelica.papyrus.ui.action3"
               label="Generate Modelica code"
               menubarPath="edu.ufc.femtost.disc.sysml4modelica.papyrus.ui.menu.sysml4modelica/SysML4ModelicaSeparator">
         </action>
      </objectContribution>
   </extension>

</plugin>
