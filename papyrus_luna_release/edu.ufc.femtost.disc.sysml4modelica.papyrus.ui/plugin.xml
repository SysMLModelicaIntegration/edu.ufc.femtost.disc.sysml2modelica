<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            id="edu.ufc.femtost.disc.sysml4modelica.papyrus.ui.objectContribution1"
            nameFilter="*.uml"
            objectClass="org.eclipse.core.resources.IFile">
         <menu
               icon="icons/SysML4ModelicaProfile.png"
               id="edu.ufc.femtost.disc.sysml4modelica.papyrus.ui.menu.sysml4modelica"
               label="SysML4Modelica"
               path="additions">
            <separator
                  name="SysML4ModelicaSeparator">
            </separator>
         </menu>
         <action
               class="edu.ufc.femtost.disc.sysml4modelica.papyrus.ui.actions.CheckModel"
               id="edu.ufc.femtost.disc.sysml4modelica.papyrus.ui.checkSysMLModel"
